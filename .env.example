# Google AI API Key (for Gemini model)
GOOGLE_API_KEY=your-gemini-api-key

# Google Cloud Configuration (for BigQuery)
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# Model Configuration
ANALYST_MODEL=gemini-2.5-flash  # Upgraded: better reasoning than 2.0-flash

# Planning/Thinking Configuration (for complex analysis)
ENABLE_PLANNING=true           # Enable extended thinking for multi-step analysis
THINKING_BUDGET=8192           # Tokens allocated for reasoning (default: 8192)

# BigQuery Configuration
BIGQUERY_DATASET=your_default_dataset  # Optional: default dataset to use
BIGQUERY_MAX_RESULTS=10000             # Max rows to return from queries

# Local Data Directory
DATA_DIR=./data
